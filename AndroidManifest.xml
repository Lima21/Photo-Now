<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.goncl.editarphoto">

    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.hardware.camera.autofocus" />
    <uses-permission android:name="android.permission.BLUETOOTH" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyAdPFkitCkohXo2n_XXQemuoL8UKpEaYlA" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".fotoFinal"
            android:theme="@android:style/Theme.Material" />
        <activity
            android:name=".GridViewActivity"
            android:theme="@android:style/Theme.Material" />
        <activity android:name=".MapsMarkerActivity" />
        <activity
            android:name=".FullScreenViewActivity"
            android:theme="@android:style/Theme.Material" />
        <activity
            android:name=".Activity_Crop"
            android:theme="@android:style/Theme.Material" />
        <activity
            android:name=".Criar_Categoria"
            android:theme="@android:style/Theme.Material" />
        <activity
            android:name=".CamTestActivity"
            android:screenOrientation="landscape" />
    </application>

</manifest>